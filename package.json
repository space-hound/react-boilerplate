{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A react app boilerplate project with webpack.",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"---------------------------1---------------------------": "--------------------------------------------------",
		"build": "webpack --config webpack.prod.js --color --progress",
		"build:dev": "webpack --config webpack.dev.js --color --progress",
		"watch": "webpack serve --config webpack.dev.js",
		"watch:prod": "webpack serve --config webpack.prod.js",
		"---------------------------2---------------------------": "--------------------------------------------------",
		"lint": "eslint --ext js,jsx src",
		"lint:fix": "eslint --fix --ext js,jsx src",
		"prettify": "prettier --check src",
		"prettify:fix": "prettier --write src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/space-hound/react-boilerplate.git"
	},
	"keywords": [
		"react",
		"webpack",
		"boilerplate",
		"javascript",
		"scss",
		"babel",
		"starter"
	],
	"author": "Borz Alexandru Mihai",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/space-hound/react-boilerplate/issues"
	},
	"homepage": "https://github.com/space-hound/react-boilerplate#readme",
	"dependencies": {
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.4.0",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.19.1",
		"@babel/preset-env": "^7.19.1",
		"@babel/preset-react": "^7.18.6",
		"babel-loader": "^8.2.5",
		"babel-plugin-styled-components": "^2.0.7",
		"compression-webpack-plugin": "^10.0.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^4.1.0",
		"eslint": "8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.8",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.1",
		"prettier": "^2.7.1",
		"sass": "^1.54.9",
		"sass-loader": "^13.0.2",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.6",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.0"
	}
}
